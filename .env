# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=http://localhost:5000

# Google Maps API
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# App Configuration
REACT_APP_APP_NAME=GeoTrack
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_REAL_TIME_TRACKING=true
REACT_APP_ENABLE_EMERGENCY_FEATURES=true
REACT_APP_ENABLE_PAYMENT_INTEGRATION=true

# SSL Commerce (for payments)
REACT_APP_SSL_COMMERCE_STORE_ID=your-store-id
REACT_APP_SSL_COMMERCE_IS_SANDBOX=true
